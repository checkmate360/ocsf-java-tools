{
  "desc": "Translates Windows 4625 (failed logon).",

  "when": "EventID = '4625'",

  "rules": [
    {
      "include": "common.json"
    },
    {
      "include": "activity.json"
    },
    {
      "include": "microsoft/windows/windows.json"
    },
    {
      "include": "microsoft/windows/XmlWinEventLog/Security/rule-462x.json"
    },

    {
      "message": {
        "@value": "An account failed to log on."
      }
    },
    {
      "EventData.FailureReason": {
        "@move": "status_detail"
      }
    }
  ]
}
