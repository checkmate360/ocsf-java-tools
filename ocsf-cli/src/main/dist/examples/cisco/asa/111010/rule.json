{
  "desc": "Translates Cisco ASA-5-111010 (A user made a configuration change).",
  "when": "code = 111010",
  "parser": {
    "name": "message",
    "pattern": "User '#{username}', running '#{application}' from IP #{ip_addr}, executed '#{cmd}'",
    "output": "event_data"
  },

  "rules": [
    {
      "@include": [
        "schema_version.json",
        "cisco/asa/common.json"
      ]
    },
    {
      "host": {
        "@move": "device.ip"
      }
    },
    {
      "event_data.username": {
        "@move": "actor.user.name"
      }
    }
  ]
}
