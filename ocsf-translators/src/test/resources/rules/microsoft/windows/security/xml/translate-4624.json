{
  "desc": "Translates Windows 4624 (successfully logon).",

  "when": "EventID = '4624'",

  "rules": [
    {
      "include": "common.json"
    },
    {
      "include": "activity.json"
    },
    {
      "include": "microsoft/windows/windows.json"
    },
    {
      "include": "microsoft/windows/security/xml/translate-462x.json"
    },

    {
      "message": {
        "@value": "An account was successfully logged on."
      }
    }
  ]
}
